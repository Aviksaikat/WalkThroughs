                <script type="text/javascript">
    var ERRORTRACKING_ENABLED =  false;

    var ERRORTRACKING_ENABLE = function() {
        var errortracking_url_coreui = "https://be7a53389fb045b48d6c384bb8ee89eb@o118517.ingest.sentry.io/1374096";
        var errortracking_unique_id = "a865915a-8205-4e66-a925-a2195103d20a";

        Sentry.init({
            dsn: errortracking_url_coreui,
            release: VERSION,
            allowUrls: [
                // only include errors raise in HTML, packed core & client
                "/static/webassets/packed_client.js?d473dbdc",
                "/static/webassets/packed_core.js?edf33a5b",
                "/i18n/en/messages.js"
            ]
        });
        Sentry.setUser({"id": errortracking_unique_id});
        ERRORTRACKING_ENABLED = true;
        log.info("Initialized error tracking");
    };

    $(function() {
        if (ERRORTRACKING_ENABLED) {
            ERRORTRACKING_ENABLE();
        }
    });
</script>
            
                
